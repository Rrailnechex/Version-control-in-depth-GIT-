from addition import addition
from division import division
from multiplication import multiplication
from substraction import substraction
import random

all_functions_list = [addition, division, multiplication, substraction]

x = 60
y = 4
results = []

for i in range(20):
    deal = random.choice(all_functions_list)
    t = deal(x, y)
    print(t)
    results.append(t)
